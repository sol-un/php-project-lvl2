{
  "name": "hexlet/code",
  "description": "Takes two json/yml/ini files and creates a customizable visual diff",
  "type": "utility",
  "autoload": {
    "files": [
      "src/Differ.php",
      "src/Builder.php",
      "src/Parser.php",
      "src/Formatters/Index.php",
      "src/Formatters/Stylish.php",
      "src/Formatters/Plain.php",
      "src/Formatters/Json.php"
    ]
  },
  "authors": [
    {
      "name": "S. J."
    }
  ],
  "require": {
    "docopt/docopt": "^1.0",
    "illuminate/collections": "^8.62",
    "symfony/yaml": "^5.3"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.99",
    "squizlabs/php_codesniffer": "^3.6",
    "phpunit/phpunit": "^9.5"
  }
}